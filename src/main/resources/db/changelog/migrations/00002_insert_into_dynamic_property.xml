<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
                      http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                      http://www.liquibase.org/xml/ns/pro             http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">

    <property name="table.name" value="core_dynamic_property" global="true"/>

    <property name="now" value="now()" dbms="mysql"/>
    <changeSet author="Cristian Ferretti" id="00002_insert_into_${table.name}">
        <preConditions onFail="MARK_RAN" onFailMessage="Table ${table.name} doesn't exists">
            <tableExists tableName="${table.name}"/>
            <and>
                <rowCount expectedRows="0" tableName="${table.name}"/>
            </and>
        </preConditions>
        <insert tableName="${table.name}">
            <column name="created_at" value="${now}"/>
            <column name="updated_at" value="${now}"/>
            <column name="prop_key" value="test1"/>
            <column name="prop_key" value="test1"/>
            <column name="content" value="test"/>
            <column name="since_version" value="1"/>
        </insert>
    </changeSet>
</databaseChangeLog>