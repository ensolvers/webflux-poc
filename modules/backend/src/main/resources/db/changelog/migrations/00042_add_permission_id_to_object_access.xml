<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
  xmlns:pro="http://www.liquibase.org/xml/ns/pro" 
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd 
                      http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                      http://www.liquibase.org/xml/ns/pro             http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">

  <property  name="table.name"  value="core_object_access" global="false"/>

  <changeSet  author="Diego Prince"  id="00042_add_permission_id_to_${table.name}_table">
    <preConditions onFail="MARK_RAN" onFailMessage="Column ${table.name}:permission_id already exists" >
      <not> <columnExists tableName="${table.name}" columnName="permission_id" /> </not>
    </preConditions>
    <addColumn  tableName="${table.name}" >
      <column  name="permission_id" type="${type.id}"> <constraints foreignKeyName="FK_${table.name}__core_permission_id" referencedTableName="core_permission" referencedColumnNames="id"/> </column>
    </addColumn>
  </changeSet>
</databaseChangeLog>