<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
  xmlns:pro="http://www.liquibase.org/xml/ns/pro"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
                      http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                      http://www.liquibase.org/xml/ns/pro             http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">

  <changeSet author="Nicolas Preisz" id="00059_rename_core_task_to_core_flow">
    <preConditions onFail="MARK_RAN" onFailMessage="Table core_flow already exists">
      <not><tableExists tableName="core_flow" /></not>
    </preConditions>
    <renameTable oldTableName="core_task" newTableName="core_flow" />
  </changeSet>
</databaseChangeLog>
