<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
                      http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                      http://www.liquibase.org/xml/ns/pro             http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">

    <property  name="table.name"  value="core_object_changelog" global="false"/>

    <changeSet  author="José Matías Rivero"  id="00040_add_ip_to_object_changelog_table">
        <preConditions onFail="MARK_RAN" onFailMessage="Column ${table.name}:ip already exists" >
            <not> <columnExists tableName="${table.name}" columnName="ip" /> </not>
        </preConditions>
        <addColumn  tableName="${table.name}" >
            <column  name="ip" type="VARCHAR(63)" />
        </addColumn>
    </changeSet>
</databaseChangeLog>