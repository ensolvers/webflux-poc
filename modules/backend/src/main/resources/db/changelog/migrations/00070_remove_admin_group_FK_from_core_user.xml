<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
                      http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                      http://www.liquibase.org/xml/ns/pro             http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">

    <property  name="table.name"  value="core_user" global="false"/>

    <changeSet  author="Diego Prince"  id="00070_remove_admin_group_FK_from_${table.name}_table">
        <preConditions onFail="MARK_RAN" onFailMessage="admin_group_id column on ${table.name} not exist" >
            <columnExists tableName="${table.name}" columnName="admin_group_id" />
        </preConditions>
        <dropForeignKeyConstraint
                baseTableName="${table.name}"
                constraintName="FK_${table.name}_core_social_content_group"
        />
        <dropColumn tableName="${table.name}">
            <column name="admin_group_id" />
        </dropColumn>
    </changeSet>
</databaseChangeLog>