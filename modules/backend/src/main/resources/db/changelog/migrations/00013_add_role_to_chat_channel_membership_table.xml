<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
                      http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                      http://www.liquibase.org/xml/ns/pro             http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">

    <property  name="table.name"  value="core_chat_channel_membership" global="false"/>

    <changeSet  author="emanuel s"  id="00013_add_role_to_${table.name}_table">
        <preConditions onFail="MARK_RAN" onFailMessage="Column ${table.name}:role already exists" >
            <not> <columnExists tableName="${table.name}" columnName="role" /> </not>
        </preConditions>
        <addColumn  tableName="${table.name}" >
            <column  name="role"       type="varchar(40)"> <constraints nullable="false" /> </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>
