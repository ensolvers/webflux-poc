<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
                      http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                      http://www.liquibase.org/xml/ns/pro             http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">

    <property  name="table.name"  value="core_chat_channel_membership" global="false"/>

    <changeSet  author="giuliano"  id="00015_fix_last_accessed_in_${table.name}_table">
        <preConditions onFail="MARK_RAN" onFailMessage="Column ${table.name}:last_accessed doesn't exists" >
            <columnExists tableName="${table.name}" columnName="last_accessed" />
        </preConditions>

        <dropNotNullConstraint tableName="${table.name}" columnName="last_accessed" columnDataType="datetime" />
    </changeSet>
</databaseChangeLog>