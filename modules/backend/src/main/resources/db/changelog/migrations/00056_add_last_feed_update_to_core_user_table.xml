<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
                      http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                      http://www.liquibase.org/xml/ns/pro             http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">

    <property  name="table.name"  value="core_user" global="false"/>
    <changeSet  author="Martin Villarreal"  id="00056_add_last_feed_update_to_${table.name}_table">
        <preConditions onFail="MARK_RAN" onFailMessage="Column last_feed_update already exists" >
            <not>
                <or>
                    <columnExists tableName="${table.name}" columnName="last_feed_update" />
                </or>
            </not>
        </preConditions>
        <addColumn tableName="${table.name}">
            <column name="last_feed_update" type="datetime">
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>